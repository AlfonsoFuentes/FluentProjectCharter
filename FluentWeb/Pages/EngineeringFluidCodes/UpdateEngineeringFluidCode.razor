@page "/UpdateEngineeringFluidCode/{Id:guid}"

<EditTemplate @ref=EditForm TItem="UpdateEngineeringFluidCodeRequest" @bind-Model="@Model"
                  Title="Create Organization Strategy">
    <Content>
        <TextBox Label="Code"
                 @bind-Value=@Model.Code
                 Placeholder="Define Code"
                 Validate="EditForm.ValidateAsync"></TextBox>
        <ValidationMessage For="() => Model.Code" style="color:red; font-size:x-small;" />
        <TextBox Label="Name"
                 @bind-Value=@Model.Name
                 Placeholder="Define name"
                 Validate="EditForm.ValidateAsync"></TextBox>
        <ValidationMessage For="() => Model.Name" style="color:red; font-size:x-small;" />
    </Content>

</EditTemplate>




@code {
    EditTemplate<UpdateEngineeringFluidCodeRequest> EditForm = null!;
}
