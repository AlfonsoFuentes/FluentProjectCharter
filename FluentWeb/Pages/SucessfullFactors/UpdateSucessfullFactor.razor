@page "/UpdateSucessfullFactor/{Id:guid}/{ProjectId:guid}"
<EditTemplate @ref=EditForm TItem="UpdateSucessfullFactorRequest" @bind-Model=@Model 
              Title="Edit SucessfullFactor">
    <Content>
        <TextArea Label="Name"
                  @bind-Value=@Model.Name
                  Placeholder="Define name"
                  Validate="EditForm.ValidateAsync"></TextArea>
        <ValidationMessage For="() => Model.Name" style="color:red; font-size:x-small;" />
    </Content>

</EditTemplate>

@code {
    EditTemplate<UpdateSucessfullFactorRequest> EditForm = null!;
}
