<FluentTabs ShowActiveIndicator="false" Style="width:100%" @bind-ActiveTabId=@Parent.Tab OnTabChange="HandleOnTabChange">
    <FluentTab Label="Deliverables" Id="Deliverables">
        <Content>
            <DeliverableList Parent="@App.Project.CurrentCase!.CurrentScope" GetAll="GetAll"></DeliverableList>
        </Content>

    </FluentTab>



    <FluentTab Label="Requirements" Id="Requirements">
        <Content>
            <RequirementList GetAll="GetAll" Parent="@App.Project.CurrentCase!.CurrentScope"></RequirementList>
        </Content>

    </FluentTab>
    <FluentTab Label="Assumptions" Id="Assumptions">
        <Content>
            <AssumptionList GetAll="GetAll" Parent="@App.Project.CurrentCase!.CurrentScope"></AssumptionList>
        </Content>

    </FluentTab>
    <FluentTab Label="Risks" Id="Risks">
        <Content>
            <DeliverableRiskList GetAll="GetAll" Parent="@App.Project.CurrentCase!.CurrentScope"></DeliverableRiskList>
        </Content>

    </FluentTab>
    <FluentTab Label="Constrainsts" Id="Constrainsts">
        <Content>
            <ConstraintList GetAll="GetAll" Parent="@App.Project.CurrentCase!.CurrentScope"></ConstraintList>
        </Content>

    </FluentTab>
    <FluentTab Label="Bennefits" Id="Bennefits">
        <Content>
            <BennefitList GetAll="GetAll" Parent="@App.Project.CurrentCase!.CurrentScope"></BennefitList>
        </Content>

    </FluentTab>
    <FluentTab Label="Acceptance Criterias" Id="AcceptanceCriteria">
        <Content>
            <AcceptanceCriteriaList GetAll="GetAll" Parent="@App.Project.CurrentCase!.CurrentScope"></AcceptanceCriteriaList>
        </Content>

    </FluentTab>
</FluentTabs>

